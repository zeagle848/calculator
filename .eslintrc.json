{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:cypress/recommended", "eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "ignorePatterns": "/dist/bundle.js",
  "rules": {
    "no-shadow": "off",
    "linebreak-style": 0,
    "quotes": 0,
    "comma-dangle": "off",
    "no-alert": "off",
    "no-restricted-globals": "off",
    "import/prefer-default-export": "off"
  }
}
